- if user_signed_in?
  %li
    = link_to t('user.edit'), edit_user_registration_path
  %li
    = link_to t('user.logout'), destroy_user_session_path, :method=>'delete'
- else
  %li
    = link_to t('user.sign_in'), new_user_session_path
  %li
    = link_to t('user.sign_up'), new_user_registration_path
